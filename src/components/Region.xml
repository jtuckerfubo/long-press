<component name="Region" extends="Rectangle">
    <interface>
        <field id="labelText" type="string" alias="label.text" />
        <function name="onKeyEvent"/>
    </interface>
    <children>
        <Label 
            id="label"
            font="font:LargeBoldSystemFont"
            color="#000000"
            horizAlign="center"
            vertAlign="center"
        />
    </children>
</component>